{
  "name": "waktos",
  "version": "3.0.0",
  "description": "~5kb for the death of ugly dates!",
  "keywords": [
    "waktos",
    "date",
    "time",
    "datetime",
    "i18n"
  ],
  "homepage": "https://github.com/fauziralpiandi/waktos#readme",
  "bugs": {
    "url": "https://github.com/fauziralpiandi/waktos/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fauziralpiandi/waktos.git"
  },
  "license": "MIT",
  "author": "Fauzira Alpiandi <fauziralpiandi.dev@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./locale/*": {
      "types": "./dist/locale/*.d.ts",
      "import": "./dist/locale/*.js",
      "require": "./dist/locale/*.cjs"
    },
    "./plugin/*": {
      "types": "./dist/plugin/*.d.ts",
      "import": "./dist/plugin/*.js",
      "require": "./dist/plugin/*.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "locale/*": [
        "dist/locale/*.d.ts"
      ],
      "plugin/*": [
        "dist/plugin/*.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "bench": "vitest bench --run",
    "build": "bash scripts/builder.sh",
    "bundle": "node scripts/bundle-analyzer.js",
    "format": "prettier --write \"**/*.{js,ts,md}\"",
    "lint": "bash scripts/linter.sh",
    "prepublishOnly": "npm run build",
    "test": "vitest --run"
  },
  "browserslist": [
    "Chrome >= 85",
    "Firefox >= 79",
    "Safari >= 14.7",
    "Edge >= 85",
    "iOS >= 14.7",
    "Android >= 85",
    "not IE 11",
    "not dead",
    "not op_mini all"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "semantic-release": "^25.0.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vitest": "^4.0.16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
